{
	"type": "project",
	"require": {
		"php": ">=5.6.0",
		"orchestra/foundation": "3.1.*",
		"orchestra/avatar": "~3.1",
		"orchestra/control": "~3.1",
		"orchestra/tenanti": "~3.1"
	},
	"require-dev": {
		"orchestra/installer": "~3.1",
		"orchestra/studio": "~3.1",
		"orchestra/testing": "~3.1",
		"mockery/mockery": "0.9.*",
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
		"stackie/adminlte-theme": "~0.1"
	},
	"autoload": {
		"classmap": [
			"resources/database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan publish:assets orchestra/foundation",
			"php artisan orchestra:optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan publish:assets orchestra/foundation",
			"php artisan orchestra:optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
